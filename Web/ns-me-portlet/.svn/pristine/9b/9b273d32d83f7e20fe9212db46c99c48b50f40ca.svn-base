		<aui:input cssClass="input1001 searchFormDatePicker" type="text" name="<%=VeSinhGiaDinhTerms.NGAY_KIEM_DEM %>" label="<%= LanguageUtil.get(pageContext, \"ngay-kiem-dem\") + dotLabel %>" ></aui:input>
	</aui:column>
	
