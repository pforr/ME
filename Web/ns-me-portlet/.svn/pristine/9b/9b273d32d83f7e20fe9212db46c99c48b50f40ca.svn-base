	<%-- <aui:column>
		<aui:input cssClass="input1001" type="text" name="ngayNhap" label="<%= LanguageUtil.get(pageContext, \"ngay-nhap-lieu\") + dotLabel %>" ></aui:input>
	</aui:column> --%>
	
